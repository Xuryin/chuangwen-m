.table-staff
  margin  0 20px
  overflow auto

  .table-staff-title
    width 100%
    height 72px
    line-height 72px
    display flex
    justify-content center
    background-color #F0F3F7

    >span
      text-align left

    >span:nth-child(1)
      width 65%
      padding-left  20px

    >span:nth-child(2)
      width 35%

.low-table-staff-content
  border 1px solid #E1E1E5

  .table-staff-content
    display flex
    flex-direction row
    justify-content center
    border-bottom 1px solid #E1E1E5

    >span
      font-size: 28px;
      color: #0F0F0F;
      line-height 50px
      height 50px
      text-align left
      padding-left 20px

    >span:nth-child(1)
      width 65%

    >span:nth-child(2)
      width 35%