.title-container {
  display: flex;
  align-items: center;
  height: 42px;
  margin: 38px 40px 34px 0;

  .title-status {
    width: 16px;
    height: 16px;
    background-color: #2D7ECD;
    border-radius: 8px;
    margin-right: 11px;
  }

  .title-text {
    flex: 1;
    font-family: PingFangSC-Medium;
    font-size: 30px;
    color: #000000;
  }
}